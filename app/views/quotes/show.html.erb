<p id="notice"><%= notice %></p>
<div class="panel panel-primary">
  <div class="panel-heading">
    <%= @quote.author %>
  </div>
  
  <div class="panel-body">
    "<%= @quote.quote %>"
  </div>
  
  <div class="panel-footer">
    <strong>Background:</strong>
    <%= @quote.background %>
  </div>
</div>
<%= link_to 'Edit', edit_quote_path(@quote), class: "btn btn-primary btn-small" %> |
<%= link_to 'Back', quotes_path, class: "btn btn-success btn-small" %>
